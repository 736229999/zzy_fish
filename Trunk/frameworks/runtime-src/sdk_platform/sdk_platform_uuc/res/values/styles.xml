<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="AppBaseTheme" parent="android:Theme.Light"></style>

    <style name="ResourceDetailsLabel">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="ResourceDetailsLabel.Large">
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/text_large_color</item>
    </style>

    <style name="ResourceDetailsLabel.Small">
        <item name="android:singleLine">true</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/text_small_color</item>
    </style>

    <!-- 定义button的长宽 @dimen/button_height -->
    <style name="btn_style">
        <item name="android:textAppearance">@android:style/TextAppearance.Medium</item>
        <item name="android:layout_height">@dimen/sdk_button_height</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_marginBottom">@dimen/button_margin</item>
        <item name="android:layout_marginTop">@dimen/button_margin</item>
        <item name="android:layout_gravity">center</item>
    </style>

    <!-- 定义textview的高度和宽度 -->
    <style name="text_base_style">
        <item name="android:textSize">@dimen/text_size</item>
        <item name="android:lineSpacingExtra">@dimen/text_line_space</item>
        <item name="android:layout_marginTop">@dimen/text_margin</item>
        <item name="android:layout_marginBottom">@dimen/text_margin</item>
    </style>

    <style name="ResourcesLoadingProgress" parent="@android:style/Widget.ProgressBar">
        <item name="android:minHeight">20dp</item>
        <item name="android:maxHeight">20dp</item>
        <item name="android:minWidth">20dp</item>
        <item name="android:maxWidth">20dp</item>
        <item name="android:indeterminateDrawable">@drawable/loading_row</item>
    </style>

    <style name="common_input_style">
        <item name="android:background">@drawable/sdk_input_bg</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/input_layout_height</item>
        <item name="android:layout_marginLeft">@dimen/input_layout_margin</item>
        <item name="android:layout_marginRight">@dimen/input_layout_margin</item>
    </style>

    <style name="bonusDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.7</item>
    </style>

    <style name="input_layout_style">
        <item name="android:background">#f5f5f5</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/sdk_balance_container_height</item>
    </style>

    <style name="input_edit_text_style" parent="android:Widget.EditText">
        <item name="android:background">@null</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/content_generic_xmedium_size</item>
        <item name="android:textColorHint">@color/color_hint_text</item>
